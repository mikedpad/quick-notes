{
  "name": "quick-notes",
  "version": "0.5.0",
  "description": "A simple example of virtual post-it notes using SvelteKit",
  "author": "Michael Daniel Padilla <michaeldpadilla@gmail.com> (https://mikedpad.com)",
  "license": "MIT",
  "homepage": "https://mikedpad.github.io/quick-notes",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mikedpad/quick-notes.git"
  },
  "type": "module",
  "scripts": {
    "start": "npm run dev -- --https --host --open",
    "dev": "svelte-kit dev",
    "build": "svelte-kit build",
    "package": "svelte-kit package",
    "preview": "npm run build && svelte-kit preview --host",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "prepare": "npm run smui-theme",
    "smui-theme": "smui-theme compile static/smui.css -i src/theme"
  },
  "keywords": [
    "post-it",
    "notes",
    "svelte",
    "sveltekit"
  ],
  "devDependencies": {
    "@mdi/js": "^6.4.95",
    "@smui/button": "^5.0.0-beta.7",
    "@smui/card": "^5.0.0-beta.7",
    "@smui/common": "^5.0.0-beta.7",
    "@smui/fab": "^5.0.0-beta.7",
    "@smui/icon-button": "^5.0.0-beta.7",
    "@smui/layout-grid": "^5.0.0-beta.7",
    "@sveltejs/adapter-static": "^1.0.0-next.28",
    "@sveltejs/kit": "^1.0.0-next.287",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-svelte3": "^3.4.1",
    "gh-pages": "^3.2.3",
    "prettier": "^2.5.1",
    "prettier-plugin-svelte": "^2.6.0",
    "sass": "^1.42.1",
    "smui-theme": "^5.0.0-beta.7",
    "svelte": "^3.46.4",
    "svelte-check": "^2.4.5",
    "svelte-preprocess": "^4.10.4",
    "tslib": "^2.3.1",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "normalize.css": "^8.0.1"
  }
}

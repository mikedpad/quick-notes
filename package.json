{
  "name": "quick-notes",
  "version": "0.3.0",
  "author": "Michael Daniel Padilla <michaeldpadilla@gmail.com> (https://mikedpad.com)",
  "license": "MIT",
  "homepage": "https://mikedpad.github.io/quick-notes",
  "repository": {
    "type": "git",
    "url": "https://github.com/mikedpad/quick-notes.git"
  },
  "description": "A simple example of virtual post-it notes using React and Greensock Animation",
  "scripts": {
    "start": "react-scripts start",
    "lint": "npm run lint:eslint && npm run lint:prettier",
    "lint:eslint": "eslint 'src/**/*.{js,jsx}'",
    "lint:prettier": "prettier --check 'src/**/*.{js,jsx}'",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "eject": "react-scripts eject"
  },
  "keywords": [
    "post-it",
    "notes",
    "react"
  ],
  "dependencies": {
    "@material-ui/core": "^4.9.5",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/styles": "^4.9.0",
    "gsap": "^3.2.4",
    "lorem-hipsum": "0.1.7",
    "notistack": "^0.9.8",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-helmet": "^6.0.0-beta.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.4.0",
    "shortid": "^2.2.15"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^2.5.0",
    "gh-pages": "^2.2.0",
    "prettier": "^1.19.1"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}

{
  "name": "quick-notes",
  "version": "0.3.2",
  "author": "Michael Daniel Padilla <michaeldpadilla@gmail.com> (https://mikedpad.com)",
  "license": "MIT",
  "homepage": "https://mikedpad.github.io/quick-notes",
  "repository": {
    "type": "git",
    "url": "https://github.com/mikedpad/quick-notes.git"
  },
  "description": "A simple example of virtual post-it notes using React and Greensock Animation",
  "scripts": {
    "start": "react-scripts start",
    "lint": "npm run lint:eslint && npm run lint:prettier",
    "lint:eslint": "eslint 'src/**/*.{js,jsx}'",
    "lint:prettier": "prettier --check 'src/**/*.{js,jsx}'",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "eject": "react-scripts eject"
  },
  "keywords": [
    "post-it",
    "notes",
    "react"
  ],
  "dependencies": {
    "@material-ui/core": "^4.11.4",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/styles": "^4.11.4",
    "lorem-hipsum": "^0.1.7",
    "notistack": "^1.0.9",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.3",
    "shortid": "^2.2.16"
  },
  "devDependencies": {
    "eslint": "^7.27.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "gh-pages": "^3.2.0",
    "prettier": "^2.3.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}

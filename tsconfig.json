{
  "extends": "./.svelte-kit/tsconfig.json",
  "compilerOptions": {
    "lib": ["es2020", "DOM", "DOM.Iterable"],
    "paths": {
      "$lib": ["src/lib"],
      "$lib/*": ["src/lib/*"],
      "$data/*": ["src/data/*"],
      "$images/*": ["src/images/*"],
      "$types/*": ["src/types/*"]
    },
    "noUnusedLocals": true,
    "noUnusedParameters": true
  }
}
